{
  "manifest_version": 3,
  "name": "Rat of Time",
  "version": "1.0",
  "description": "テキストボックスに自動で値を入力します",
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "action": {
    "default_title": "自動入力する",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "config.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}