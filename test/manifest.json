{
  "name": "Multi Function Example",
  "version": "1.0",
  "manifest_version": 3,
  "description": "複数関数で構成された拡張機能の例",
  "permissions": ["scripting", "activeTab"],
  "action": {
    "default_title": "クリックして実行"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["utils.js"],
      "run_at": "document_idle"
    }
  ]
}