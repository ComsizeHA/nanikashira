{
  "manifest_version": 3,
  "name": "Atai Keeper",
  "version": "1.0",
  "description": "Save and view values using a popup",
  "permissions": ["storage", "tabs"],
  "action": {
    "default_popup": "view.html",
    "default_title": "Atai Keeper"
  },
  "background": {
    "service_worker": "background.js"
  }
}